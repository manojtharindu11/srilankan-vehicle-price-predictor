URL=https://*****.com
USE_SELENIUM=1
HEADLESS=1
# Fetch each ad page for more fields (Contact, Make/Model, Engine, Details, Images)
SCRAPE_DETAILS=1
# Stop after collecting this many ads total (across all pages)
MAX_ITEMS=200
# Optional:
# START_PATH=/search/cars/toyota
# MAX_PAGES=5
# REQUEST_DELAY=1

# Logging
LOG_LEVEL=INFO
# LOG_LEVEL=DEBUG
# LOG_FILE=scrape.log

# Selenium/Browser noise suppression (hides Chromium stderr spam)
SILENCE_BROWSER_LOGS=1